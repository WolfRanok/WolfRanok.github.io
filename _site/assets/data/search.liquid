hrome()\n\n    def click_object(self, object):\n        &quot;&quot;&quot;\n        ç”¨äºç‚¹å‡»ä¸€ä¸ªå…ƒç´ \n        :param object:å…ƒç´ å¯¹è±¡\n        :return: None\n        &quot;&quot;&quot;\n        self.browser.execute_script(&quot;arguments[0].click();&quot;, object)\n\n    def close_ad(self):\n        # è¿™é‡Œæ˜¾ç¤ºç­‰å¾…ç•Œé¢ä¸­çš„å¹¿å‘Šå‡ºç°\n        try:\n            self.ad = WebDriverWait(self.browser, 1).until(EC.presence_of_element_located((By.XPATH, self.path_ad)))\n        except:\n            return\n        self.click_object(self.ad)\n\n    def __init__(self, debug=False):\n        self.browser = self.get_browser() if debug else self.get_no_ui_browser()  # è·å–ä¸€ä¸ªæµè§ˆå™¨å¯¹è±¡\n        self.browser.implicitly_wait(5)  # è®¾ç½®éšå¼ç­‰å¾…çš„æ—¶é—´\n\n        self.browser.get(self.url)  # æ‰“å¼€ç•Œé¢\n        self.close_ad()  # å…³é—­ç•Œé¢ä¸Šçš„å¹¿å‘Š\n        self.find_button()  # å¯»æ‰¾è¾“å…¥æ¡†\n\n    def find_button(self):\n        &quot;&quot;&quot;\n        æ­¤æ–¹æ³•ç”¨äºå®ç°å¸¸ç”¨å…ƒç´ çš„æŸ¥æ‰¾\n        :return: None\n        &quot;&quot;&quot;\n        ## å¯»æ‰¾è¾“å…¥è¾“å‡ºæ¡†æ¡†\n        self.text_input = WebDriverWait(self.browser, 5).until(\n            EC.presence_of_element_located((By.XPATH, self.path_input)))\n        # self.text_out = self.browser.find_element(by=By.XPATH, value=self.path_out)\n\n    def input(self, text):\n        &quot;&quot;&quot;\n        ç”¨äºç»™è¾“å…¥æ¡†è¾“å…¥å¥å­\n        :param text: å¾…ç¿»è¯‘æ–‡æœ¬\n        :return: bool\n        &quot;&quot;&quot;\n        self.text_input.clear()  # æ¸…é™¤è¾“å…¥å†…å®¹\n        self.text_input.send_keys(text)  # å¡«å†™è¢«ç¿»è¯‘å†…å®¹\n\n    def get_translate_sentence(self):\n        &quot;&quot;&quot;\n        è·å–ç¿»è¯‘ç»“æœ\n        :return:ç¿»è¯‘ç»“æœ\n        &quot;&quot;&quot;\n        try:\n            self.text_out = WebDriverWait(self.browser, 5).until(EC.presence_of_element_located((By.XPATH, self.path_out)))\n        except:\n            return &#39;&#39;\n        return self.text_out.text\n\n    def translate_sentence(self, text, wait=0.5):\n        &quot;&quot;&quot;\n        ç”¨äºå®ç°ç¿»è¯‘çš„é€»è¾‘\n        :param text: éœ€è¦ç¿»è¯‘çš„æ–‡æœ¬\n        :return: ç¿»è¯‘ç»“æœ\n        &quot;&quot;&quot;\n        self.input(text)\n        time.sleep(wait)\n        return self.get_translate_sentence()\n\n\nâ€ƒâ€ƒåœ¨åŠ ä¸Šå¤šçº¿ç¨‹ä¼˜åŒ–\nclass translation_queue:\n    number = 5  # é»˜è®¤å¼€å¯ç•Œé¢æ•°\n    queue = []  # æµè§ˆå™¨é˜Ÿåˆ—\n    hh, tt = 0, 0  # é˜Ÿåˆ—å¤´ï¼Œé˜Ÿåˆ—å°¾\n\n    def add_browser(self):\n        self.queue.append(spider())\n\n    def __init__(self, number=None):\n        ## åˆå§‹åŒ–çº¿ç¨‹æ± å¤§å°\n        if number is not None:\n            self.number = number\n\n        ## åˆå§‹åŒ–5ä¸ªæµè§ˆå™¨\n        children = []\n        for _ in range(self.number):\n            children.append(Thread(target=self.add_browser))\n            children[-1].start()\n\n        # ç­‰å¾…æ‰€æœ‰æµè§ˆå™¨åˆå§‹åŒ–å®Œæ¯•\n        for i in range(self.number):\n            children[i].join()\n\n    def set_text(self, text):\n        self.queue[self.tt].input(text)\n        self.tt = (self.tt + 1) % self.number\n\n    def get_text(self):\n        text = self.queue[self.hh].get_translate_sentence()\n        self.hh = (self.hh + 1) % self.number\n        return text\n\n\n\n\nä½œè€…è‡ªå·±çš„ä¸€äº›è¯\n\nâ€ƒâ€ƒæœ¬é¡¹ç›®å…¶å®åªæ˜¯å¯¹äººå·¥æ™ºèƒ½ç®—æ³•æ¨¡å‹SVMåˆ†ç±»å™¨çš„ä¸€ä¸ªå°å°çš„åº”ç”¨ã€‚ä¸€ä¸ªæ¨¡å‹èƒ½åšçš„äº‹æƒ…å…¶å®éƒ½å¾ˆå°å¾ˆç®€å•ï¼Œå› æ­¤å¯¹å›¾ç‰‡çš„æ•°æ®æ¸…æ´—è§£æç®€åŒ–ç­‰çš„æ­¥éª¤å°¤ä¸ºé‡è¦ï¼Œè€Œè¿™äº›å¤„ç†æ­¥éª¤å…¶å®å°±æ˜¯ï¼Œå°†æˆ‘ä»¬æ‰€èƒ½çœ‹æ‡‚å›¾ç‰‡ä¿¡æ¯ä¸€æ­¥æ­¥è½¬åŒ–æˆè®¡ç®—æœºèƒ½è¯»çš„æ‡‚çš„æ•°æ®ã€‚ç„¶åå°†è®¡ç®—æœºå¾—åˆ°çš„ç»“æœè½¬åŒ–æˆæˆ‘ä»¬èƒ½è¯»æ‡‚çš„ä¿¡æ¯ï¼ˆå¦‚å›¾ç‰‡ï¼Œæ–‡å­—ï¼Œè§†é¢‘ï¼‰ã€‚è¿™äº›è½¬åŒ–çš„ä»»åŠ¡æ­£æ˜¯æˆ‘ä»¬ç¨‹åºå‘˜çš„å·¥ä½œã€‚\n\nâ€ƒâ€ƒåœ¨é¡¹ç›®å®æ“ä¸­ï¼Œè¿˜æ˜¯é‡åˆ°äº†ä¸å°‘çš„å›°éš¾ï¼Œä¾‹å¦‚è®­ç»ƒæ•°æ®æ‰¾ä¸åˆ°ï¼Œè®­ç»ƒæ•°æ®æ´—ä¸å¹²å‡€ï¼Œæ¨¡å‹æ•ˆç‡ä½ï¼Œå­—æ¯ilåˆ†ä¸æ¸…ï¼Œä¼ ç»Ÿçˆ¬è™«çˆ¬ä¸äº†ç¿»è¯‘ç­‰ç­‰é—